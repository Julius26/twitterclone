<% provide(:title, 'Sign Up')%>
<h1>Sign Up</h1>


<div class="row">
<div class="col-md-6 col-md-offset-3">

<%= form_for(@user) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.email_field :email %>
<%= f.label :password %>
<%= f.password_field :password %>
<%=f.label :password_confirmation%>
<%= f.password_field :password_confirmation%>